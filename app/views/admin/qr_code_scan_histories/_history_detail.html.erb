<%- model_class = QrCodeScanHistory -%>
<div class="table-responsive block-mt">
<table class="table table-striped">
    <tr>
      <!-- <th class="text-nowrap"><%= model_class.human_attribute_name(:id) %></th> -->
      <th class="text-nowrap"><%= model_class.human_attribute_name(:user_id) %></th>
      <th class="text-nowrap"><%= model_class.human_attribute_name(:good_id) %></th>
      <th class="text-nowrap"><%= model_class.human_attribute_name(:good_instance_id) %></th>
      <th class="text-nowrap"><%= model_class.human_attribute_name(:score) %></th>
      <th class="text-nowrap"><%= model_class.human_attribute_name(:status) %></th>
      <th class="text-nowrap"><%= model_class.human_attribute_name(:created_at) %></th>
      <th class="text-nowrap"><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  <tbody>
    <% @qr_code_scan_histories.each do |qr_code_scan_history| %>
      <tr>
        <!-- <td><%= link_to qr_code_scan_history.id, admin_qr_code_scan_history_path(qr_code_scan_history) %></td> -->
        <% if qr_code_scan_history.user.blank? %>
          <td>-</td>
        <% else %>
          <td><%= qr_code_scan_history.user.name %></td>
        <% end %>
        <td><%= qr_code_scan_history.good.name %></td>
        <td><%= qr_code_scan_history.good_instance_id %></td>
        <td><%= qr_code_scan_history.score %></td>
        <td><%if qr_code_scan_history.status == "00A" %><%="成功"%><%else%><%="失败"%><%end%></td>
        <td><%= qr_code_scan_history.created_at.strftime('%Y-%m-%d %H:%M:%S') %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      admin_qr_code_scan_history_path(qr_code_scan_history), :class => 'btn btn-default btn-xs' %>
 <!--         <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_admin_qr_code_scan_history_path(qr_code_scan_history), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      admin_qr_code_scan_history_path(qr_code_scan_history),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => '您确认删除?')) },
                      :class => 'btn btn-xs btn-danger' %> -->
        </td>
      </tr>
    <% end %>
    <div>
  </tbody>
</table>
  <%= link_to t('.new', :default => t("helpers.links.new")),
            new_admin_qr_code_scan_history_path,
            :class => 'btn btn-warning' %>
      </div>
      <%= paginate @qr_code_scan_histories %>
</div>