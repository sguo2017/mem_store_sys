<%= form_for [:admin, @red_packet_base_rate], :html => { :class => "form-horizontal red_packet_base_rate" } do |f| %>

  <% if @red_packet_base_rate.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@red_packet_base_rate.errors.count, "error") %> prohibited this red_packet_base_rate from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @red_packet_base_rate.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :catalog_i, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :catalog_i, :class => 'form-control' %>
    </div>
    <%=f.error_span(:catalog_i) %>
  </div>
  <div class="form-group">
    <%= f.label :catalog_ii, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :catalog_ii, :class => 'form-control' %>
    </div>
    <%=f.error_span(:catalog_ii) %>
  </div>
  <div class="form-group">
    <%= f.label :val, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :val, :class => 'form-control' %>
    </div>
    <%=f.error_span(:val) %>
  </div>
  <div class="form-group">
    <%= f.label :rate, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :rate, :class => 'form-control' %>
    </div>
    <%=f.error_span(:rate) %>
  </div>

  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                admin_red_packet_base_rates_path, :class => 'btn btn-default' %>
    </div>
  </div>

<% end %>
