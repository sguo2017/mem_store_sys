<%- model_class = RedPacketBaseRate -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:catalog_i) %></th>
      <th><%= model_class.human_attribute_name(:catalog_ii) %></th>
      <th><%= model_class.human_attribute_name(:val) %></th>
      <th><%= model_class.human_attribute_name(:rate) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @red_packet_base_rates.each do |red_packet_base_rate| %>
      <tr>
        <td><%= link_to red_packet_base_rate.id, admin_red_packet_base_rate_path(red_packet_base_rate) %></td>
        <td><%= red_packet_base_rate.catalog_i %></td>
        <td><%= red_packet_base_rate.catalog_ii %></td>
        <td><%= red_packet_base_rate.val %></td>
        <td><%= red_packet_base_rate.rate %></td>
        <td><%=l red_packet_base_rate.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      admin_red_packet_base_rate_path(red_packet_base_rate), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_admin_red_packet_base_rate_path(red_packet_base_rate), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      admin_red_packet_base_rate_path(red_packet_base_rate),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_admin_red_packet_base_rate_path,
            :class => 'btn btn-primary' %>
