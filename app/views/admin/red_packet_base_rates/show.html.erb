<%- model_class = RedPacketBaseRate -%>

<div class="container">
  <div class="panel">
    <div class="panel-heading">
      <div class="panel-title">
        <%=t '.title', :default => model_class.model_name.human.titleize %>
      </div>
    </div>
    <div class="panel-body">
      <dl class="dl-horizontal">
        <dt><strong><%= model_class.human_attribute_name(:catalog_i) %>:</strong></dt>
        <dd><%= @red_packet_base_rate.catalog_i %></dd>
        <dt><strong><%= model_class.human_attribute_name(:catalog_ii) %>:</strong></dt>
        <dd><%= @red_packet_base_rate.catalog_ii %></dd>
        <dt><strong><%= model_class.human_attribute_name(:val) %>:</strong></dt>
        <dd><%= @red_packet_base_rate.val %></dd>
        <dt><strong><%= model_class.human_attribute_name(:rate) %>:</strong></dt>
        <dd><%= @red_packet_base_rate.rate %></dd>
      </dl>
      <div>
        <%= link_to t('.back', :default => t("helpers.links.back")),
                admin_red_packet_base_rates_path, :class => 'btn btn-default'  %>
        <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_admin_red_packet_base_rate_path(@red_packet_base_rate), :class => 'btn btn-default' %>
        <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      admin_red_packet_base_rate_path(@red_packet_base_rate),
                      :method => 'delete',
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-danger' %>
      </div>
    </div>
  </div>
</div>




